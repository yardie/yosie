<h1>New booking</h1>

<% form_for(@booking) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :ArrivalDate %><br />
    <%= f.date_select :ArrivalDate %>
  </p>
  <p>
    <%= f.label :DepartureDate %><br />
    <%= f.date_select :DepartureDate %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', bookings_path %>